! --- Script by ilya112358@gmail.com
! --- Export Kinematics, Kinetics data and corresponding normatives from Visual3D workspace prepared by Clinical Gate PAF

Set_Pipeline_Parameter_To_Folder_Path
/PARAMETER_NAME=FOLDER
/PARAMETER_VALUE=
/FROM_MOTION_FILE_IN_WORKSPACE=Gait*.c3d
;

! --- Create EXPORT\ directory before calling the script or during the run
Set_Pipeline_Parameter_To_Folder_Path
/PARAMETER_NAME=EXPORTFOLDER
/PARAMETER_VALUE=::FOLDER&EXPORT\
;

Select_Active_File
/FILE_NAME=GLOBAL
;

! --- Folder Name to extract subsession name
Create_Text_Data
/SIGNAL_FOLDER=META
/SIGNAL_NAMES=Folder Name
/TEXT_DATA=::FOLDER
;

Export_Data_To_Ascii_File
/FILE_NAME=::EXPORTFOLDER&Info.txt
/SIGNAL_TYPES=TEXT_DATA
/SIGNAL_FOLDER=META
/SIGNAL_NAMES=First Name+Last Name+ID+DOB+Creation date+Height+Weight+Folder Name
;

! --- Temporal and Spatial
Select_Active_File
/FILE_NAME=ALL_FILES
! /QUERY=
;

Metric_Compute_Temporal_Distance
! /GLOBAL_RESULT_METRIC_FOLDER=TEMPORAL_DISTANCE
